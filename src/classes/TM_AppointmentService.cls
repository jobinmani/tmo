    // Get Store Appointment Availability List
    public static TM_Result getStoreAppointmentAvailabilityList( String storeSAPId, 
                                                                 String requestDate,
                                                                 String numberOfDays )
    {
        TM_Result result = new TM_Result();

        List<TM_Globals.StoreAppointment> appointmentList = 
                                          new List<TM_Globals.StoreAppointment>();

        String accessToken;
        if(!test.isRunningTest())
        {
            B2BAuthentication b2b = new B2BAuthentication();
            accessToken = b2b.Authenticate();
        } 
        else 
        {
            accessToken = 'test';
        }

        system.debug('accessToken' + accessToken);
        B2BEndpoint__c b2bServices = B2BEndpoint__c.getValues('Services');
        
        if( b2bServices != null && accessToken != null)
        {
            Date reqDate         = date.valueOf(requestDate);
            DateTime reqDT       = DateTime.newInstance( reqDate.year(), reqDate.month(), reqDate.day());
            String   reqDTFmt    = reqDT.format('M/d/YYYY');

            String body = '{' +
                            '"storeSAPId": "' + storeSAPId + '",' +
                            '"reqDate"   : "' + reqDTFmt + '",' +
                            '"source"    : " "' +
                           '}';
            
            System.debug('getAsString:' + body );

            HttpResponse response = TM_Util.sendJSON( b2bServices.getStoreAppointments__c,
                                                      accessToken, 
                                                      body );
            if( response.getStatusCode() == 200 )
            {
               System.debug('Success: ' + response.getBody() );
               if(!test.isRunningTest())
               {
                  //to get all day wise slots
                  String params = (String)JSON.deserializeUntyped(response.getBody()); 
           
                  Map<String, Object> tmpResult = 
                     (Map<String, Object>) JSON.deserializeUntyped(params);

                  // Get slots of date provided in input
                  List<Object> resultList = (List<Object>)(tmpResult.get('perDay'));
                  Integer           count = resultList.size();
                  
                  System.debug('Count :' + count);
                  
                  if( count > 0 )
                  { 
	                  for(integer i=0; i < count; i++)
	                  {    
	                      Map<String, Object> test3 = (Map<String, Object>)resultList[i];   
	                    
	                      String availabilityDate = (String)test3.get('availableDate');
	                      
                          System.debug(LoggingLevel.Info, 'Request Date:' + requestDate);      //  9/01/2017 value on UI
	                      System.debug(LoggingLevel.Info, 'Avail   Date:' + availabilityDate); //  9/1/2017 value from B2b
	                
                          List<Object> testList = (List<Object>)test3.get('slots'); 
                                                  
                          System.debug('INSTANCE OF ' + testList);
                          String textList = '';
                          Date availDate =  Date.parse( availabilityDate );
                          
                          if( testList != null && testList.size() > 0 )
                          {
                             DateTime availDT = DateTime.newInstance( availDate, System.now().timeGMT() );
                             DateTime endDT   = reqDT.addDays( Integer.valueOf( numberOfDays ) );
                             
                             System.debug('AvailDT: ' + availDT + ' EndDT: ' + endDT );

                             for( Object slot : testList )
                             {
                                 Integer slotTime = (Integer)slot;
                                 
                                 TM_Globals.StoreAppointment appointment = 
                                                             new TM_Globals.StoreAppointment();
                                 
                                 appointment.storeId         = storeSAPId;
                                 appointment.appointmentId   = '';
                                 appointment.appointmentDate = availabilityDate;
                                 appointment.appointmentTime = '' + slotTime;
    
                                 // Only return dates within the range
                                 // bugbug - B2B doesn't support date range                             
                                 if( availDT < endDT )
                                     appointmentList.add( appointment );
                             }
                          }
	                  }
                      result.valueObject = appointmentList;   
                  }
                  else
                  {
                      result.returnCode = TM_Result.RESULT_NO_RESULTS_FOUND;
                  }
               }
            }
            else
            {
               // Error condition
               system.debug('body==='+response.getBody());
                
               result.returnCode = TM_Result.RESULT_NO_RESULTS_FOUND;
               result.errorList.add( response.getBody() );
            }
        }
        return(result);
    }               
    
    // Return appointments by leadid
    public static TM_Result getAppointmentByLeadId( String leadId )
    {
        TM_Result result = new TM_Result();
        
        try
        {
            List<Event__x> appointmentList = 
                           [
                              select id, 
                                     type__c, 
                                     subject__c, 
                                     externalId,
                                     event_store_id_c__c,
                                     consumerlead_c__c,
                                     description_c__c,
                                     startdatetime__c,
                                     enddatetime__c,
                                     consumerlead_c__r.customer_time_zone__c
                              from Event__x
                              where consumerlead_c__c=:leadId
                              and Meeting_status_c__C='Confirmed'
                           ];
                           
            if( appointmentList != null )
            {                                
                // Adjust appointment to users local timezone
                for( Event__x appointment : appointmentList )
                {
                    // bugbug - default to PST ( unless user provided )
                    Integer timezoneDelta = -8;
                    
                    
                    if( appointment.consumerlead_c__r != null )
                    {
                        // Fetch timezone for users time (GMT -x to find timezone)
                        String tzName = appointment.consumerlead_c__r.customer_time_zone__c;

                        if( String.isNotEmpty(tzName) )
                        {
                           String offset =  TM_AppointmentService.TzToOffset.get( tzName );
                           
                           if( offset != null )
                           {
                              timezoneDelta = Integer.valueOf( offset );
                           }
                           else
                           {
                              System.debug('Offset not defined or found. Default PST');
                           }
                        }
                        else
                        {
                           System.debug('TZ Not Defined. Default PST');
                        }
                    }
                    
                    // Adjusted start/endtimes appointment to users local timezone             
                    if( appointment.startdatetime__c != null )
                        appointment.startdatetime__c = appointment.startdatetime__c.addHours( timezoneDelta );
                        
                    if( appointment.enddatetime__c != null )
                        appointment.enddatetime__c = appointment.enddatetime__c.addHours( timezoneDelta );
                }
                
                result.valueObject = appointmentList;
            }
        }
        catch( Exception e )
        {
            result.returnCode = TM_Result.RESULT_GENERAL_ERROR;       
            result.errorList.add('getAppointmentByLeadId: ' + e );
        }
        return( result );
    }
    
    // Return appointments by leadid
    public static TM_Result deleteAppointmentById( String appointmentId )
    {
        TM_Result result = new TM_Result();
        
        try
        {
             Event__x appointment = 
                           [
                              select id,
                                     store_sap_c__c,
                                     consumerlead_c__c
                              from Event__x
                              where externalId=:appointmentId
                           ];
                           
            if( appointment != null )
            {
                Lead lead = TM_LeadService.getLeadById( appointment.consumerlead_c__c );
                
                if( lead != null )
                {
                    // Delete appointment
                    Database.deleteImmediate(appointment);

                    // Update schedule lead indicator
                    // bugbug - this could be removed in the future
                    // if B2B updates and connector syncs
                    lead.is_event_Scheduled__c = false;
                    update lead;
                }
                else
                {
                    result.returnCode = TM_Result.RESULT_GENERAL_ERROR;       
                    result.errorList.add('Could not find lead to delete appointment');                    
                }
            }
            else
            {
                result.returnCode = TM_Result.RESULT_GENERAL_ERROR;       
                result.errorList.add('Appointment not found for request [' + appointmentId + ']');                    
            }
        }
        catch( Exception e )
        {
            result.returnCode = TM_Result.RESULT_GENERAL_ERROR;       
            result.errorList.add('Appointment not found: ' + e );
        }
        return( result );
    }   
    
